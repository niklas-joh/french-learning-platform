{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/app.ts",
    "start": "ts-node src/app.ts",
    "clean": "rm -rf dist",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "db:migrate": "knex migrate:latest --knexfile src/knexfile.ts",
    "db:rollback": "knex migrate:rollback --knexfile src/knexfile.ts",
    "db:seed": "knex seed:run --knexfile src/knexfile.ts",
    "db:status": "knex migrate:list --knexfile src/knexfile.ts",
    "populate:content": "ts-node -P ./tsconfig.json ../scripts/populate_content.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "morgan": "^1.10.0",
    "objection": "^3.1.5",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/knex": "^0.15.2",
    "@types/morgan": "^1.9.10",
    "@types/supertest": "^6.0.3",
    "jest": "^30.0.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
